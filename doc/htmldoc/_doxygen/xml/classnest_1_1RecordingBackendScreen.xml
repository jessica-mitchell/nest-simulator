        <reimplements refid="classnest_1_1RecordingBackend_1a930dda24c880fbe74509016c351b796a">write</reimplements>
        <param>
          <type>const <ref refid="classnest_1_1RecordingDevice" kindref="compound">RecordingDevice</ref> &amp;</type>
